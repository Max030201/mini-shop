{
  "name": "mini-shop",
  "version": "1.0.0",
  "description": "Mini Shop React App with PrimeReact",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "dev": "webpack serve --mode development",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "json-server --watch db.json --port 3001",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false --verbose",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:report": "jest --coverage --watchAll=false --coverageReporters=html --coverageReporters=text",
    "test:ci": "jest --ci --coverage --watchAll=false --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.17",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
